{
  "expo": {
    "name": "Sheddle",
    "slug": "sheddle",
    "scheme": "sheddle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/maskable_icon_alpha.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/maskable_icon_alpha.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
